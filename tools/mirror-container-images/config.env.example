# Mirror Images Configuration
# 이 파일을 config.env로 복사하여 사용하세요.
#
# cp config.env.example config.env

# ECR 설정
ECR_ACCOUNT="123456789012"
ECR_REGION="ap-northeast-2"

# 복사할 플랫폼 (쉼표 구분)
PLATFORMS="linux/amd64,linux/arm64"

# Rate limit 대응 설정
MAX_RETRIES=5
INITIAL_DELAY=10
DELAY_BETWEEN=5

# 병렬 처리 설정
PARALLEL_JOBS=4
